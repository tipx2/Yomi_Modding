[application]

config/name="Yomi Hustle"
run/main_scene="res://Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="YomiHustle"
config/icon="res://icon2.png"

[autoload]

ReplayManager="*res://ReplayManager.gd"
Global="*res://Global.gd"
ModLoader="*res://modloader/ModLoader.gd"
Network="*res://Network.gd"
SteamYomi="*res://SteamY.gd"
SteamLobby="*res://SteamLobby.gd"
Custom="*res://Custom.gd"

[debug]

gdscript/warnings/enable=false
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Holepunch/plugin.cfg", "res://addons/char_creator/plugin.cfg", "res://addons/collision_box/plugin.cfg", "res://addons/export_categories/plugin.cfg", "res://addons/multirun/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true
timers/tooltip_delay_sec=0.25

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

